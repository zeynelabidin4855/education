public class Mo1 {

    public Integer add(Integer a, Integer b){
        return a+b;
    }
    public Integer add(Integer a, Integer b, Integer c){
        return a+b+c;
    }
    public Double add(Double a, Integer b){
        return a+b;
    }
    public Double add(Integer a, Double b){
        return a+b;
    }
}